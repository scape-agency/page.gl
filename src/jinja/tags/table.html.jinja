<!-- Tables -->
<table>
    <tr>
        {% for header in table_headers %}
        <th>{{ header }}</th>
        {% endfor %}
    </tr>
    {% for row in table_rows %}
        <tr>
        {% for cell in row %}
            <td>{{ cell }}</td>
        {% endfor %}
        </tr>
    {% endfor %}
</table>
